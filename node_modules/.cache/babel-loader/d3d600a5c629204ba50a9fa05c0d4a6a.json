this.answer == undefined && this.count > 0) {\n        return;\n      }\n      if (this.count >= 2) {\n        this.count = 0;\n      }\n      this.count++;\n    },\n    handler: function(e) {\n      if (e.target.id == 0) {\n        this.answer = \"Myself\";\n      }\n      else if (e.target.id == 1) {\n        this.answer = \"My child\";\n      }\n      else if (e.target.id == 2) {\n        this.answer = \"Someone else\";\n      }\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}